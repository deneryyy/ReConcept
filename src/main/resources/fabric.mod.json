{
  "schemaVersion": 1,
  "id": "reconcept",
  "version": "${version}",
  "name": "ReConcept",
  "description": "Reconceptualizing Minecraft survival experience",
  "authors": ["Denery"],
  "contact": {
    "repo": "https://github.com/deneryyy/ReConcept",
    "issues": "https://github.com/deneryyy/ReConcept/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/reconcept/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "moe.denery.reconcept.data.ReConceptDataGenerator"
    ],
    "client": [
      "moe.denery.reconcept.client.ReConceptClient"
    ],
    "main": [
      "moe.denery.reconcept.ReConcept"
    ]
  },
  "mixins": [
    "reconcept.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Discord Server": "https://discord.com/invite/MuqxBDM8a7",
        "Youtube Channel": "https://www.youtube.com/@maestro_denery"
      },
      "update_checker": true
    }
  }
}
